'use strict';function initBlockHandler(){$(document).on('ajaxComplete',function(e,xhr){if(xhr.responseJSON&&xhr.responseJSON.blockContent){document.write(xhr.responseJSON.blockContent);document.close();}});}
function initCachedPagesVerification(){var $verifyInput=$('[name="pxVerify"]');if($verifyInput.length&&$verifyInput.val()==='true'){$.get($verifyInput.data('verify-url'),{pxUri:location.pathname});}}
$(document).ready(function(){initBlockHandler();initCachedPagesVerification();});